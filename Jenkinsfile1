pipeline {
    agent any
    tools {
        jdk 'JAVA_HOME'
        nodejs 'nodejs25'
    }
   
    stages {
       
        stage ("Git Checkout") {
            steps {
                git 'https://github.com/chandrahas02/Zomato.git'
            }
        }
         stage("Install NPM Dependencies") {
            steps {
                sh "npm install"
            }
        }
    }
}
